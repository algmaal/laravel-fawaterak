{
  "name": "algmaal/laravel-fawaterak",
  "description": "Laravel package for Fawaterak payment gateway integration",
  "type": "library",
  "keywords": [
    "laravel",
    "payment",
    "gateway",
    "fawaterak",
    "egypt",
    "payment-processing"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "algmaal",
      "email": "mohamedalgamal@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Algmaal\\LaravelFawaterak\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Algmaal\\LaravelFawaterak\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Algmaal\\LaravelFawaterak\\FawaterakServiceProvider"
      ],
      "aliases": {
        "Fawaterak": "Algmaal\\LaravelFawaterak\\Facades\\Fawaterak"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
